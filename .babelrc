{
  "env": {
    "cjs": {
      "presets": [["@babel/preset-react"]],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": true
          }
        ],
        [
          "@babel/plugin-transform-modules-commonjs",
          {
            "strict": true
          }
        ]
      ]
    },
    "webpack": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry"
          }
        ],
        ["@babel/preset-react"]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ]
      ]
    }
  }
}
